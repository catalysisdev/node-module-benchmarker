{
  "name": "node-load-benchmarking-loader",
  "version": "1.0.0",
  "description": "A Node module loader to benchmark the time it takes to load an ESM module graph",
  "keywords": [
    "benchmark",
    "esm"
  ],
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "nx exec -- tsc -p ./tsconfig.build.json",
    "clean": "rimraf dist",
    "test": "vitest --run"
  },
  "author": "oss@craftweg.com",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "vitest": "^0.26.3",
    "nx": "15.4.2",
    "@types/node": "14.17.0"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=14.17.0"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/dist"
        ]
      },
      "clean": {}
    }
  }
}
